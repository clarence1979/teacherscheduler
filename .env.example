# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google OAuth Configuration (for SSO and Calendar Integration)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Microsoft OAuth Configuration (for SSO and Outlook Calendar Integration)
VITE_MICROSOFT_CLIENT_ID=your_microsoft_client_id_here
VITE_MICROSOFT_CLIENT_SECRET=your_microsoft_client_secret_here
VITE_MICROSOFT_TENANT_ID=common

# Optional: OpenAI API Key (can also be set in app settings)
VITE_OPENAI_API_KEY=your_openai_api_key_here